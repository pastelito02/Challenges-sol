let input = """
216
38.3 36.5 52.5 43.5 46.6 62.6 58.3 46.8 46.0 49.1 34.9 35.2 22.9 24.8 24.5 15.8 21.9 11.0 9.6 10.6 9.3 16.3 24.5 37.3 30.0 35.1 39.9 39.2 57.2 49.3 56.6 54.0 44.1 49.1 35.3 35.2 34.1 15.3 21.8 8.2 21.6 18.1 10.0 -2.2 12.6 15.8 20.0 24.3 17.9 38.9 42.4 41.8 47.3 49.5 46.8 59.9 34.0 44.4 32.0 46.5 22.3 38.1 23.1 14.9 16.8 22.4 14.1 11.0 0.2 15.9 21.8 35.8 30.0 34.4 53.3 50.7 48.5 49.3 47.2 51.3 45.6 58.3 54.1 36.5 30.1 21.1 20.6 16.0 16.3 13.0 10.0 19.3 14.4 15.5 17.2 24.9 29.2 23.1 37.1 43.7 47.4 60.6 48.9 38.7 45.8 42.7 49.9 48.9 31.2 19.5 21.6 28.3 6.8 12.0 10.9 12.0 20.7 15.6 19.5 26.0 23.9 42.9 40.2 50.7 45.6 54.2 50.0 49.4 53.9 44.6 48.9 35.2 30.2 21.9 20.0 22.2 12.7 10.7 15.6 7.1 12.9 15.9 15.3 26.7 29.2 40.8 40.0 41.4 44.7 52.9 59.5 49.0 48.4 44.2 37.8 47.2 29.9 25.5 20.0 14.4 12.1 10.7 12.0 21.6 13.2 14.7 24.1 19.2 32.0 35.9 39.5 43.8 45.9 49.3 43.2 49.4 47.0 44.2 43.1 35.2 29.9 26.2 29.9 8.5 11.3 6.2 0.5 11.3 17.6 16.0 30.9 20.9 19.8 36.1 33.9 45.5 61.0 60.6 56.1 50.9 47.6 41.1 32.3 43.1 31.1 22.2 20.0 5.6 16.3 9.2 10.3 7.8 27.7 15.9 19.3 23.4
"""

let lines = input.split(separator: "\n")
let doubles = lines[1].split(separator: " ").map({ Double($0)! })
var smoothed = [Double]()
for i in 1..<doubles.count - 1 {
    smoothed.append((doubles[i - 1] + doubles[i] + doubles[i + 1]) / 3.0)
}
smoothed.insert(doubles[0], at: 0)
smoothed.append(doubles[doubles.count - 1])
let answer = smoothed.reduce("", { $0 + String($1) + " " })
print(answer.trimmingCharacters(in: .whitespaces))
